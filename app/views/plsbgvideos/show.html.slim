/p#notice = notice

p
  strong = "#{t('pls.name')}:"
  = @plsbgvideo.name
p
  strong = "#{t('pls.devices')}:"
  - @plsbgvideo.devices.each do |device|
    p #{device.id}. #{device.name}
p
  strong = "#{t('pls.files')}:"
  - @plsbgvideo.mediafiles.each do |mediafile|
    p #{mediafile.id}. #{mediafile.filename}

= link_to t('std.back'), plsbgvideos_path
